{
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "strict": true,
    "noEmit": false,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

    "strictNullChecks": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/Context/FormContext.d.tsx"],
  "exclude": ["node_modules"]
}
